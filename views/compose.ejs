<%- include("partials/header") %>
<h1>Compose</h1>
<form action="/compose" method="post">
    <h4>Title</h4>
    <input type="text" name="postTitle" autocomplete="off" class="form-control" placeholder="New Title" required>
    <h4>Post</h4>
    <textarea name="postBody" id="" cols="30" rows="10" class="form-control" placeholder="Post" required></textarea ><br>
    <button class="btn btn-success" >Publish</button >
</form>
<%- include("partials/footer") %> 

